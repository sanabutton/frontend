{
  "private": true,
  "name": "@sanabutton/player",
  "version": "0.0.0",
  "description": "さなボタン(2) のオーディオプレイヤー",
  "main": "index.js",
  "scripts": {
    "build": "parcel build index.ts",
    "pretest": "npm run lint && npm run clean",
    "test": "jest --ci",
    "lint": "npm run lint:prettier && npm run lint:tsc",
    "lint:prettier": "prettier -l \"src/**/*\"",
    "lint:tsc": "tsc -p ./tsconfig.lint.json",
    "clean": "shx rm -rf coverage dist",
    "fmt": "prettier --write \"src/**/*\""
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/node": "16.11.14",
    "jest": "27.4.5",
    "parcel-bundler": "1.12.5",
    "prettier": "2.5.1",
    "shx": "0.3.3",
    "ts-jest": "27.1.1",
    "typescript": "4.5.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "axios": "0.24.0"
  }
}
